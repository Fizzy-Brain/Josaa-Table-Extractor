{
  "manifest_version": 3,
  "name": "JOSAA Table Extractor",
  "version": "1.0",
  "description": "Extract JOSAA tables as CSV",
  "content_scripts": [
    {
      "matches": [
        "*://josaa.nic.in/*",
        "*://josaa.admissions.nic.in/*",
        "*://*.josaa.nic.in/*",
        "*://*.nic.in/*josaa*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "permissions": ["downloads"],
  "action": {
    "default_popup": "popup.html"
  }
}